collection File extends aeria.file {}

collection Pet {
  icon "dog"
    indexes {
      name
    }
  properties {
    name str
      details []{
        properties {
          picture File @accept(["image/*"])
        }
      }
  }
  functions {
    get @expose(true)
  }
  filtersPresets {
    active {
      label "Active"
        filters {
          active true
        }
    }
  }
}

